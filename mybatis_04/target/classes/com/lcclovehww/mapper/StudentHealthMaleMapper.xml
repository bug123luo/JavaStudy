<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lcclovehww.mapper.StudentHealthMaleMapper">
	<select id="findStudentHealthMaleByStuId" parameterType="int" resultType="com.lcclovehww.pojo.StudentHealthMale">
        SELECT
        id,
        stu_id as stuId,
        check_date as checkDate,
        heart,
        liver,
        spleen,
        lung,
        kidney,
        prostate,
        remark
        FROM
        student_health_male where stu_id=#{id}
    </select>
</mapper>