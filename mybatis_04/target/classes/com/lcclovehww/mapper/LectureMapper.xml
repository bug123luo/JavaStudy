<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lcclovehww.mapper.LectureMapper">
	<select id="getLecture" parameterType="long" resultType="com.lcclovehww.pojo.Lecture">
		select id,lecture_name as lectureName,remark from lecture where id=#{id}
	</select>
</mapper>