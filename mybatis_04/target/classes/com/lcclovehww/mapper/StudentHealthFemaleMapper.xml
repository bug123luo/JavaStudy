<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lcclovehww.mapper.StudentHealthFemaleMapper">
	<select id="findStudentHealthFemaleByStuId" parameterType="int"
            resultType="com.lcclovehww.pojo.StudentHealthFemale">
        SELECT
        id,
        stu_id as stuId,
        check_date as checkDate,
        heart,
        liver,
        spleen,
        lung,
        kidney,
        uterus,
        remark
        FROM
        student_health_female where stu_id=#{id}
    </select>
</mapper>