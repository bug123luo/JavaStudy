<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lcclovehww.mybatis.RoleMapper">
	<select id="getRole" parameterType="long" resultType="com.lcclovehww.jdbc.Role">
		select id, role_name as role_name, remark from role where id = #{id}
	</select>
</mapper>