<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tct.db.mapper.MessageRecordsCustomMapper">
	
	<sql id="allColumns">
		id,
		serl_num,
		message,
		gun_id
	</sql>
	
	<select id="selectBySerlNum" parameterType="MessageRecordsQueryVo" resultType="com.tct.db.po.MessageRecordsCustom">
		select <include refid="allColumns"/> from message_records
		<where>
			<if test=" messageRecordsCustom.gunImei != null">
				gun_id = #{messageRecordsCustom.gunId}
			</if>		
			<if test=" messageRecordsCustom.serlNum !=null">
				serl_num = #{messageRecordsCustom.serlNum}
			</if>
		</where>
	</select>
	
	<insert id="insertSelective" parameterType="com.tct.db.po.MessageRecordsCustom">
	    insert into message_records
	    <trim prefix="(" suffix=")" suffixOverrides=",">
	      <if test="id != null">
	        id,
	      </if>
	      <if test="serlNum != null">
	        serl_num,
	      </if>
	      <if test="message != null">
	        message,
	      </if>
	      <if test="gunId != null">
	        gun_id,
	      </if>
	    </trim>
	    <trim prefix="values (" suffix=")" suffixOverrides=",">
	      <if test="id != null">
	        #{id,jdbcType=INTEGER},
	      </if>
	      <if test="serlNum != null">
	        #{serlNum,jdbcType=CHAR},
	      </if>
	      <if test="message != null">
	        #{message,jdbcType=VARCHAR},
	      </if>
	      <if test="gunImei != null">
	        #{gunId,jdbcType=CHAR},
	      </if>
	    </trim>
	</insert>
</mapper>