<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tct.db.mapper.AppCustomMapper">
	<sql id="selectColumn">
		id,
		app_system,
		app_system_version,
		app_phone_type_name,
		app_name,
		app_mac,
		app_imei,
		app_phone,
		app_readable_code,
		app_type,
		version
	</sql>
	
	<sql id="selectAppImeiColumn">
		app_imei
	</sql>
	
	<select id="selectAppAllColumn" parameterType="com.tct.db.po.AppCustomQueryVo" resultType="com.tct.db.po.AppCustom">
		select <include refid="selectColumn"/> from app
		<where>
			<if test="appCustom.appImei != null">
				app_imei=#{appCustom.appImei}
			</if>
		</where>
	</select>
</mapper>